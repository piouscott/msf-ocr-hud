{
  "manifest_version": 2,
  "name": "MSF Screenshot",
  "version": "1.0",

  "background": {
    "scripts": ["bg.js"]
  },

  "browser_action": {
    "default_title": "Screenshot MSF"
  },

  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"],
    "run_at": "document_end",
    "all_frames": true
  }],

  "permissions": [
    "activeTab",
    "tabs",
    "webNavigation",
    "<all_urls>"
  ]
}